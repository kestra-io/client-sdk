{
  "name": "{{{projectName}}}",
  "version": "{{{projectVersion}}}",
  "description": "{{{projectDescription}}}",
  "license": "{{licenseName}}",
  "type": "module",
  "scripts": {
    "build": "tsdown && tsc -p tsconfig.json --declaration --emitDeclarationOnly --declarationMap",
    "dev": "tsdown --watch",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "browser": {
    "fs": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kestra-io/client-sdk.git"
  },
{{#npmRepository}}
  "publishConfig":{
    "registry":"{{npmRepository}}"
  },
{{/npmRepository}}
  "exports": {
    ".": "./dist{{#invokerPackage}}/{{.}}{{/invokerPackage}}/KestraClient.mjs",
    "./package.json": "./package.json"
  },
  "main": "./dist{{#invokerPackage}}/{{.}}{{/invokerPackage}}/KestraClient.mjs",
  "module": "./dist{{#invokerPackage}}/{{.}}{{/invokerPackage}}/KestraClient.mjs",
  "types": "./dist{{#invokerPackage}}/{{.}}{{/invokerPackage}}/KestraClient.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "superagent": "^5.3.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "tsdown": "^0.17.0",
    "typescript": "^5.9.3"
  }
}
