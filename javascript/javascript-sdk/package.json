{
  "name": "@kestra-io/kestra-sdk",
  "version": "1.0.10",
  "description": "All API operations, except for Superadmin-only endpoints, require a tenant identifier in the HTTP path.<br/> Endpoints designated as Superadmin-only are not tenant-scoped.",
  "license": "Unlicense",
  "type": "module",
  "scripts": {
    "build": "tsdown && tsc -p tsconfig.json --declaration --emitDeclarationOnly --declarationMap",
    "dev": "tsdown --watch",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "browser": {
    "fs": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kestra-io/client-sdk.git"
  },
  "exports": {
    ".": "./dist/KestraClient.mjs",
    "./package.json": "./package.json"
  },
  "main": "./dist/KestraClient.mjs",
  "module": "./dist/KestraClient.mjs",
  "types": "./dist/KestraClient.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "superagent": "^5.3.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "tsdown": "^0.17.0",
    "typescript": "^5.9.3"
  }
}
